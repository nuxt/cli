{
  "name": "nuxi",
  "version": "3.10.0",
  "description": "⚡️ Nuxt Generation CLI Experience",
  "repository": "nuxt/cli",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": "./dist/index.mjs",
    "./cli": "./bin/nuxi.mjs",
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.mjs"
    },
    "./utils/banner": {
      "types": "./dist/utils/banner.d.ts",
      "import": "./dist/utils/banner.mjs"
    },
    "./utils/cjs": {
      "types": "./dist/utils/cjs.d.ts",
      "import": "./dist/utils/cjs.mjs"
    },
    "./utils/console": {
      "types": "./dist/utils/console.d.ts",
      "import": "./dist/utils/console.mjs"
    },
    "./utils/dev": {
      "types": "./dist/utils/dev.d.ts",
      "import": "./dist/utils/dev.mjs"
    },
    "./utils/engines": {
      "types": "./dist/utils/engines.d.ts",
      "import": "./dist/utils/engines.mjs"
    },
    "./utils/env": {
      "types": "./dist/utils/env.d.ts",
      "import": "./dist/utils/env.mjs"
    },
    "./utils/esm": {
      "types": "./dist/utils/esm.d.ts",
      "import": "./dist/utils/esm.mjs"
    },
    "./utils/fs": {
      "types": "./dist/utils/fs.d.ts",
      "import": "./dist/utils/fs.mjs"
    },
    "./utils/kit": {
      "types": "./dist/utils/kit.d.ts",
      "import": "./dist/utils/kit.mjs"
    },
    "./utils/nuxt": {
      "types": "./dist/utils/nuxt.d.ts",
      "import": "./dist/utils/nuxt.mjs"
    },
    "./utils/packageManagers": {
      "types": "./dist/utils/packageManagers.d.ts",
      "import": "./dist/utils/packageManagers.mjs"
    },
    "./utils/templates": {
      "types": "./dist/utils/templates.d.ts",
      "import": "./dist/utils/templates.mjs"
    },
    "./utils/update": {
      "types": "./dist/utils/update.d.ts",
      "import": "./dist/utils/banner.mjs"
    }
  },
  "types": "./dist/index.d.ts",
  "bin": {
    "nuxi": "./bin/nuxi.mjs",
    "nuxi-ng": "./bin/nuxi.mjs",
    "nuxt": "./bin/nuxi.mjs",
    "nuxt-cli": "./bin/nuxi.mjs"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "build:stub": "unbuild --stub",
    "dev": "node ./bin/nuxi.mjs dev ./playground",
    "dev:bun": "bun --bun ./bin/nuxi.mjs dev ./playground",
    "lint": "eslint . && prettier --check src",
    "lint:fix": "eslint --fix . && prettier --write src",
    "nuxi": "node ./bin/nuxi.mjs",
    "nuxi-bun": "bun --bun ./bin/nuxi.mjs",
    "prepack": "unbuild",
    "release": "pnpm test && changelogen --release && npm publish && git push --follow-tags",
    "test": "pnpm lint && pnpm test:types && pnpm build && pnpm test:dist",
    "test:dist": "node ./bin/nuxi.mjs info ./playground",
    "test:types": "tsc --noEmit"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxt/kit": "^3.8.1",
    "@nuxt/schema": "^3.8.1",
    "@nuxt/test-utils": "^3.8.1",
    "@types/http-proxy": "^1.17.14",
    "@types/node": "^20.9.2",
    "@types/semver": "^7.5.5",
    "@types/ws": "^8.5.9",
    "c12": "^1.5.1",
    "changelogen": "^0.5.5",
    "chokidar": "^3.5.3",
    "citty": "^0.1.5",
    "clipboardy": "^4.0.0",
    "colorette": "^2.0.20",
    "consola": "^3.2.3",
    "destr": "^2.0.2",
    "eslint": "^8.54.0",
    "execa": "^8.0.1",
    "fuse.js": "^7.0.0",
    "giget": "^1.1.3",
    "h3": "^1.9.0",
    "httpxy": "^0.1.5",
    "jiti": "^1.21.0",
    "listhen": "^1.5.5",
    "magicast": "^0.3.2",
    "mlly": "^1.4.2",
    "nuxt": "^3.8.1",
    "nypm": "^0.3.3",
    "ofetch": "^1.3.3",
    "pathe": "^1.1.1",
    "perfect-debounce": "^1.0.0",
    "pkg-types": "^1.0.3",
    "prettier": "^3.1.0",
    "scule": "^1.1.0",
    "semver": "^7.5.4",
    "unbuild": "^2.0.0",
    "unws": "^0.2.4",
    "ws": "^8.14.2"
  },
  "optionalDependencies": {
    "fsevents": "~2.3.3"
  },
  "packageManager": "pnpm@8.10.5",
  "engines": {
    "node": "^14.18.0 || >=16.10.0"
  }
}