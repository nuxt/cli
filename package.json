{
  "name": "nuxi-ng",
  "version": "0.0.1",
  "repository": "nuxt/nuxt",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": "./dist/index.mjs",
    "./cli": "./bin/nuxi.mjs"
  },
  "types": "./dist/index.d.ts",
  "bin": {
    "nuxi": "./bin/nuxi.mjs",
    "nuxi-ng": "./bin/nuxi.mjs",
    "nuxt": "./bin/nuxi.mjs",
    "nuxt-cli": "./bin/nuxi.mjs"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "build:stub": "unbuild --stub",
    "nuxi-ng": "JITI_ESM_RESOLVE=1 jiti ./src/cli-run.ts",
    "dev": "pnpm run nuxi-ng dev ./playground",
    "lint": "eslint . && prettier --check src",
    "lint:fix": "eslint --fix . && prettier --write src",
    "prepack": "unbuild",
    "release": "pnpm test && changelogen --release && npm publish && git push --follow-tags",
    "test": "pnpm build && pnpm test:dist",
    "test:dist": "node ./bin/nuxi.mjs info"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.1.1",
    "@nuxt/kit": "^3.6.1",
    "@nuxt/schema": "^3.6.1",
    "@types/clear": "^0.1.2",
    "@types/flat": "^5.0.2",
    "@types/http-proxy": "^1.17.11",
    "@types/mri": "^1.1.1",
    "@types/node": "^20.3.2",
    "@types/semver": "^7.5.0",
    "c12": "^1.4.2",
    "changelogen": "^0.5.3",
    "chokidar": "^3.5.3",
    "clear": "^0.1.0",
    "clipboardy": "^3.0.0",
    "colorette": "^2.0.20",
    "consola": "^3.2.2",
    "deep-object-diff": "^1.1.9",
    "destr": "^2.0.0",
    "eslint": "^8.43.0",
    "execa": "^7.1.1",
    "flat": "^5.0.2",
    "fuse.js": "^6.6.2",
    "get-port-please": "^3.0.1",
    "giget": "^1.1.2",
    "h3": "^1.7.1",
    "http-proxy": "^1.18.1",
    "jiti": "^1.18.2",
    "listhen": "^1.0.4",
    "magicast": "^0.2.9",
    "mlly": "^1.4.0",
    "mri": "^1.2.0",
    "nuxt": "^3.6.1",
    "nypm": "^0.2.2",
    "ofetch": "^1.1.1",
    "pathe": "^1.1.1",
    "perfect-debounce": "^1.0.0",
    "pkg-types": "^1.0.3",
    "prettier": "^2.8.8",
    "scule": "^1.0.0",
    "semver": "^7.5.3",
    "unbuild": "^1.2.1"
  },
  "optionalDependencies": {
    "fsevents": "~2.3.2"
  },
  "packageManager": "pnpm@8.6.5",
  "engines": {
    "node": "^14.18.0 || >=16.10.0"
  }
