{
  "name": "nuxi",
  "version": "3.8.1",
  "description": "⚡️ Nuxt Generation CLI Experience",
  "repository": "nuxt/cli",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": "./dist/index.mjs",
    "./cli": "./bin/nuxi.mjs"
  },
  "types": "./dist/index.d.ts",
  "bin": {
    "nuxi": "./bin/nuxi.mjs",
    "nuxi-ng": "./bin/nuxi.mjs",
    "nuxt": "./bin/nuxi.mjs",
    "nuxt-cli": "./bin/nuxi.mjs"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "build:stub": "unbuild --stub",
    "dev": "node ./bin/nuxi.mjs dev ./playground",
    "lint": "eslint . && prettier --check src",
    "lint:fix": "eslint --fix . && prettier --write src",
    "nuxi": "node ./bin/nuxi.mjs",
    "prepack": "unbuild",
    "release": "pnpm test && changelogen --release && npm publish && git push --follow-tags",
    "test": "pnpm lint && pnpm test:types && pnpm build && pnpm test:dist",
    "test:dist": "node ./bin/nuxi.mjs info ./playground",
    "test:types": "tsc --noEmit"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxt/kit": "npm:@nuxt/kit-edge",
    "@nuxt/schema": "npm:@nuxt/schema-edge",
    "@types/clear": "^0.1.2",
    "@types/flat": "^5.0.2",
    "@types/http-proxy": "^1.17.11",
    "@types/mri": "^1.1.1",
    "@types/node": "^20.6.0",
    "@types/semver": "^7.5.1",
    "@types/ws": "^8.5.5",
    "c12": "^1.4.2",
    "changelogen": "^0.5.5",
    "chokidar": "^3.5.3",
    "citty": "^0.1.4",
    "clear": "^0.1.0",
    "clipboardy": "^3.0.0",
    "colorette": "^2.0.20",
    "consola": "^3.2.3",
    "deep-object-diff": "^1.1.9",
    "destr": "^2.0.1",
    "eslint": "^8.49.0",
    "execa": "^8.0.1",
    "flat": "^5.0.2",
    "fuse.js": "^6.6.2",
    "giget": "^1.1.2",
    "h3": "^1.8.1",
    "http-proxy": "^1.18.1",
    "httpxy": "^0.1.5",
    "jiti": "^1.20.0",
    "listhen": "^1.5.0",
    "magicast": "^0.2.10",
    "mlly": "^1.4.2",
    "mri": "^1.2.0",
    "nitropack": "^2.6.3",
    "nuxt": "^3.7.1",
    "nypm": "^0.3.2",
    "ofetch": "^1.3.3",
    "pathe": "^1.1.1",
    "perfect-debounce": "^1.0.0",
    "pkg-types": "^1.0.3",
    "prettier": "^3.0.3",
    "scule": "^1.0.0",
    "semver": "^7.5.4",
    "unbuild": "^2.0.0",
    "unws": "^0.2.4",
    "ws": "^8.14.1"
  },
  "optionalDependencies": {
    "fsevents": "~2.3.3"
  },
  "packageManager": "pnpm@8.7.4",
  "engines": {
    "node": "^14.18.0 || >=16.10.0"
  }
}